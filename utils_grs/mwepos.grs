package mwepos_put {
  rule fix2 (feature $l0, $l1, $mwepos; file "lexicons/mwe2.lex") {
    pattern {
      H[!mwepos, lemma=$l0];
      H -[fixed]-> F1; F1[lemma=$l1];
    }
    without { H -[fixed]-> F }
    commands { H._MISC_mwepos = $mwepos }
  }


  rule fix3 (feature $l0, $l1, $l2, $mwepos; file "lexicons/mwe3.lex") {
